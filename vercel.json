{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/collect-market-data",
      "schedule": "0 22 * * *"
    }
  ],
  "env": {
    "NODE_VERSION": "20"
  }
}
